{
  "name": "cknow/checker",
  "description": "A PHP code quality tool with git hooks and custom commands.",
  "type": "composer-plugin",
  "keywords": [
    "code quality",
    "code style",
    "git hooks",
    "custom commands",
    "code",
    "quality",
    "style",
    "git",
    "hooks",
    "custom",
    "commands",
    "lint",
    "linter",
    "validate",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce"
  ],
  "homepage": "https://github.com/cknow/checker",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Gobbo de Souza",
      "email": "ricardogobbosouza@yahoo.com.br",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "ricardo@clicknow.com.br",
    "issues": "https://github.com/cknow/checker/issues",
    "source": "https://github.com/cknow/checker"
  },
  "require": {
    "php": ">=5.5.9",
    "composer-plugin-api": "~1.0",
    "composer/composer": "~1.0",
    "doctrine/collections": "~1.3",
    "gitonomy/gitlib": "~1.0",
    "monolog/monolog": "~1.21",
    "symfony/config": "~3.0",
    "symfony/console": "~3.0",
    "symfony/dependency-injection": "~3.0",
    "symfony/event-dispatcher": "~3.0",
    "symfony/filesystem": "~3.0",
    "symfony/finder": "~3.0",
    "symfony/options-resolver": "~3.0",
    "symfony/process": "~3.0",
    "symfony/proxy-manager-bridge": "~3.0",
    "symfony/yaml": "~3.0"
  },
  "require-dev": {
    "mockery/mockery": "0.9.5",
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "ClickNow\\": "src/ClickNow"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "extra": {
    "class": "ClickNow\\Checker\\Composer\\CheckerPlugin"
  },
  "bin": [
    "bin/checker"
  ]
}
